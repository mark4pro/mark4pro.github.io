@!pengoTemplate=module!@
<div id='mainView' class='modal-menu-body aside cart' >
 <div class='toolbarView'>
    <div class='toolbar-dummy'></div>
        <div id='closeCartBut' class='toolbar-remove'></div>
    </div>
    <h1 id='title'></h1>
    <div id='container'>
        <div class='modal-form-divTable-shop gvgContainer'>
            <div id='main-items' class='modal-menu-options '>
                <p id='error' class='error-text'></p>
                <p id='empty' class='modal-menu-option-noprojs error-text'></p>
            </div>
            <div class='gvg-total sc-tool-text horizontal'>
                <div id='checkoutSubTotal' class='item-name'></div>
                <div id='cart-subtotal' class='item-price'></div>
            </div>
            <div class='gvg-total sc-tool-text horizontal'>
                <div id='checkoutTaxes' class='item-name'></div>
                <div id='cart-taxes' class='item-price'></div>
            </div>
            <div class='gvg-total sc-tool-text horizontal'>
                <div id='checkoutTotal' class='item-name'></div>
                <div id='cart-total' class='item-price'></div> 
            </div>
        </div>
        <div class='action-bar'>
            <input id='btnPay' type='button' class='modal-form-button type1' onclick='' value='Pay with Paypal'/>
            <span class='gvg-paypal'></span>
            <p id='errorContainerPay' class='error-text'></p>
        </div>
        <p id='errorContainer' class='error-text'></p>
    </div>
</div>
<div class='modal-menu-body background-side'>
    <div id='pauseText'>PAUSED</div>
</div>
@!pengoTemplate!@

