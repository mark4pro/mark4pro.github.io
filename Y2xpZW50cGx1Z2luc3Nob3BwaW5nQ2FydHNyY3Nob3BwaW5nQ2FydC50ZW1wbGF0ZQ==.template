@!pengoTemplate=item!@
<div class='item'>
    <div class='sc-tool-image'></div>
    <div class='sc-tool-text'>
       <div class='item-name'></div>
       <div class='item-desc'></div>
    </div>
    <div class='gvgAmount'>
        <div id='vgAdd' class='vgMore'>+</div>
        <div id='vgRemove' class='vgLess'>-</div>
    </div>
    <div class='gvgBuy has-tooltip'>
        <div id='price' class='item-price'></div>
        <div id='vgQuantity' class='item-quantity'>1</div>
        <i class='icon-shoppingcart '> </i>
        <span class='help' id='buyVGLabel'></span>
    </div>
</div>
@!pengoTemplate!@

@!pengoTemplate=itemFtm!@
<div class='item ftm'>
    <div class='sc-tool-image'></div>
    <div class='sc-tool-text'>
       <div class='item-name'></div>
       <div class='item-desc'></div>
    </div>
    <div class='gvgBuy has-tooltip'>
        <div id="payByMobile">Pay by <strong>MOBILE</strong><span class='icon-pay-mobile '></span></div>
    </div>
</div>
@!pengoTemplate!@

@!pengoTemplate=paymentMethod!@
    <input type='checkbox'/><img/>
@!pengoTemplate!@

@!pengoTemplate=toolbarHome!@
    <span id='homeButton' class='toolbar-button-home'/>
@!pengoTemplate!@

@!pengoTemplate=notification!@
    <p id='notificationContainer' class='notification-text ko'>Virtual Goods purchased successfully</p>
@!pengoTemplate!@